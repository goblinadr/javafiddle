<faces-config xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"              
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
       http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd" version="2.0">
    <managed-bean>
        <managed-bean-name>registrationBean</managed-bean-name>
        <managed-bean-class>com.javafiddle.web.beans.RegistrationBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>loginBean</managed-bean-name>
        <managed-bean-class>com.javafiddle.web.beans.LoginBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>userProjectsBean</managed-bean-name>
        <managed-bean-class>com.javafiddle.web.beans.UserProjectsBeanWeb</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
</faces-config>